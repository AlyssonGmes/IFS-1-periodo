public class Questao1 {
    public static void main(String[] args) {
        int n = 82, soma = 0;

        while(n<953){
            if(n%2 != 0){
                soma += n; //estava fora, mas entendi que deveria somar todos os valores mostrados
                System.out.println(n);
            }
            n++;
        }
        System.out.println("\nSomatÃ³rio: "+soma);
    }
}
